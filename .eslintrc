{
  "extends": "eslint-config-airbnb",
  "rules": {
    "react/jsx-indent-props": [2, "tab"],
    "indent": [2, "tab"],
    "id-length": [1, { "min": 1 }],
    "strict": [0, "global"],
    "comma-dangle": 0,
    "consistent-return": 0,
    "max-len": [2, 300, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }]
  }
}
